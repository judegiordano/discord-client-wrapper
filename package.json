{
  "name": "discord-client-wrapper",
  "version": "1.1.9",
  "description": "A Ts Discord Client API Wrapper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "eslint --fix --ext .ts,.tsx .",
    "build": "tsc -p .",
    "prestart": "npm run build",
    "start": "node .",
    "release": "npm run build && npm publish",
    "package": "npm run build && npm pack",
    "package-light": "npm run build && npm pack --dry-run",
    "test": "jest"
  },
  "keywords": [
    "discord",
    "typescript",
    "node.js",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/judegiordano/discord-client-wrapper"
  },
  "author": "Jude Giordano",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
